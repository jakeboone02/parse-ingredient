  "keywords": [
    "parse",
    "recipe",
    "ingredient",
    "quantity",
    "number"
  ],
  "bugs": {
    "url": "https://github.com/jakeboone02/parse-ingredient/issues"
  },
  "homepage": "https://github.com/jakeboone02/parse-ingredient",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeboone02/parse-ingredient"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc",
